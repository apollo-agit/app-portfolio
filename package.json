{
  "name": "app-portfolio",
  "version": "1.0.0",
  "description": "Web and mobile application for site portfolio",
  "author": "Rebel Technologies, Inc.",
  "homepage": "https://gitlab.com/rebeltechnologies/app-portfolio/wikis/home",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/rebeltechnologies/app-portfolio.git"
  },
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules doc typings coverage dist",
    "clean:dist": "rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached --config config/webpack.config.dev.js",
    "server": "npm run server:dev:hmr",
    "server:dev": "webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "start": "npm run server"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.5.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.25.0",
    "es7-reflect-metadata": "^1.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "rimraf": "^2.5.4",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.7",
    "@angular/compiler": "2.0.0-rc.7",
    "@angular/core": "2.0.0-rc.7",
    "@angular/forms": "2.0.0-rc.7",
    "@angular/http": "2.0.0-rc.7",
    "@angular/platform-browser": "2.0.0-rc.7",
    "@angular/platform-browser-dynamic": "2.0.0-rc.7",
    "@angular/router": "3.0.0-rc.3",
    "@angular/upgrade": "2.0.0-rc.7",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.21"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
